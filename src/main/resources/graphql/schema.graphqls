type Member {
    id: ID!
    firstName: String!
    lastName: String!
    group: Groups!
}

type Groups {
    groupId: ID!
    name: String!
    leadName: String
}
type Query {
    membersByGroup(name: String): [Member]!
    allMembers: [Member]!

}

type Mutation {
    createMember(name: String, lastName: String): Member!
}